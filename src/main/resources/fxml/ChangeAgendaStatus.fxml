<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="pt.ipp.isep.dei.esoft.project.application.controller.GUIController.ChangeStatusAgendaGUI"
            prefHeight="400.0" prefWidth="600.0">

    <ComboBox fx:id="comboBox" prefWidth="150"/>

    <Region VBox.vgrow="ALWAYS" />

    <HBox spacing="10" alignment="CENTER">
        <Button text="Button 1" onAction="#handleCanceledStatus"/>
        <Button text="Button 2" onAction="#handlePostponedStatus"/>
    </HBox>

</AnchorPane>
