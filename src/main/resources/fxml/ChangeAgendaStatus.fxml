<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="pt.ipp.isep.dei.esoft.project.application.controller.GUIController.ChangeStatusAgendaGUI"
            prefHeight="400.0" prefWidth="600.0">

    <VBox spacing="10" alignment="CENTER">
        <ComboBox fx:id="comboBox" prefWidth="150"/>
        <Button text="Cancel Task" onAction="#handleCanceledStatus"/>
        <Button text="Postpone Task" onAction="#handlePostponedStatus"/>
    </VBox>

</AnchorPane>
